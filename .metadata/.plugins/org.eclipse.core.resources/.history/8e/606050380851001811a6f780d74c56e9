#include <iostream>
#include <stack>
#include <queue>
using namespace std;

class Test {
	string name;

public:
	Test(string name) : name(name) {

	}
};

int main() {

	// LIFO
	stack<Test> testStack;

	testStack.push(Test("Mike"));
	testStack.push(Test("John"));
	testStack.push(Test("Sue"));



	return 0;
}
