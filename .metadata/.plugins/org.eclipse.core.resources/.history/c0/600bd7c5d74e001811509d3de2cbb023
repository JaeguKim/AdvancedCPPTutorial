#include <iostream>
#include <map>

using namespace std;

int main() {
	map<string, int> ages;

	ages["Mike"] = 40;
	ages["Raj"] = 20;
	ages["Vicky"] = 30;

	cout << ages["Raj"] << endl;

	return 0;
}
