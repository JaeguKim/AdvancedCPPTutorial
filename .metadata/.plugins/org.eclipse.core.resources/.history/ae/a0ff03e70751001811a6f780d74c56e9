#include <iostream>
#include <stack>
#include <queue>
using namespace std;

class Test {
	int id;
	string name;

public:
	Test(): id(0), name(""){

	}

	Test(int id, string name): id(id), name(name) {

	}

	void print() const {
		cout << id << ": " << name << endl;
	}

	bool operator<(const Test &other) const {
		if (name == other.name)
		{
			return id < other.id;
		}
		else
			return name < other.name;

	}
};

int main() {
	stack<Test> testStack;
	return 0;
}
